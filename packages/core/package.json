{
  "name": "@tranjs/core",
  "version": "1.0.18",
  "description": "Node.js transaction management framework inspired by Spring's `@Transactional` annotation.",
  "scripts": {
    "build": "tsc -b",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asheswook/tranjs.git"
  },
  "keywords": [
    "transaction",
    "transactional",
    "typeorm",
    "orm",
    "database",
    "query"
  ],
  "engines": {
    "node": ">=16.4.0"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "author": "asheswook",
  "license": "LGPL-2.1-only",
  "bugs": {
    "url": "https://github.com/asheswook/tranjs/issues"
  },
  "homepage": "https://github.com/asheswook/tranjs#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.5.0"
  },
  "gitHead": "7eff63661a41cb6adecdafb88eea4adb7726ef61",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "text",
      "html"
    ],
    "moduleDirectories": [
      "<rootDir>",
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "coverage"
  }
}
